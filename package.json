{
  "name": "InteractiveShell",
  "version": "1.2.0",
  "description": "Build web apps for interactive command-line tools",
  "contributors": [
    {
      "name": "Franziska Hinkelmann",
      "email": "franziska.hinkelmann@gmail.com"
    },
    {
      "name": "Lars Kastner",
      "email": "kastner@math.fu-berlin.de"
    },
    {
      "name": "Mike Stillman",
      "email": "mikestillman1@gmail.com"
    }
  ],
  "engine": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "D": "0.0.1",
    "async": "0.9.0",
    "chai": "2.2.0",
    "cookie": "0.3.1",
    "create-download-link": "1.1.0",
    "dialog-polyfill": "0.4.3",
    "exports-loader": "0.6.3",
    "express": "4.13.4",
    "express-winston": "0.2.9",
    "forever": "0.14.1",
    "get-selected-text": "1.0.2",
    "http-auth": "2.3.1",
    "imports-loader": "0.6.5",
    "jquery": "2.2.3",
    "scroll-down": "2.0.0",
    "serve-favicon": "2.1.7",
    "serve-static": "1.7.2",
    "set-caret-position": "3.0.0",
    "socket.io": "2.0.3",
    "socket.io-client": "2.0.3",
    "socketio-file-upload": "0.4.6",
    "ssh2": "0.5.5",
    "winston": "0.8.3"
  },
  "scripts": {
    "start": "webpack && tsc && node src/dist/index.js",
    "debug-start": "webpack --config webpack.debug-config.js && tsc && node src/dist/index.js",
    "basic": "webpack && tsc && node src/dist/index.js M2 docker",
    "local": "webpack && tsc && node src/dist/index.js M2 local",
    "twoMachines": "webpack && tsc && node src/dist/index.js M2 ssh",
    "twoMachines_forever": "webpack && tsc && ./node_modules/forever/bin/forever start -l /home/ubuntu/webm2.logs/forever.log -o /home/ubuntu/webm2.logs/out.log -e /home/ubuntu/webm2.logs/err.log --append src/dist/startupConfigs/Macaulay2SshDocker.js",
    "start_singular": "webpack && tsc && node src/dist/index.js singular docker",
    "basic_singular": "webpack && tsc && node src/dist/index.js singular local",
    "ssh_singular": "webpack && tsc && node src/dist/index.js singular ssh",
    "forever_singular": "webpack && ./node_modules/forever/bin/forever start -l /home/user/logs/forever.log -o /home/user/logs/out.log -e /home/user/logs/err.log --append src/startupConfigs/SingularSshDocker.js",
    "stop": "kill `ps ax | grep Server | grep -v monitor | grep node | awk '{print $1}'`",
    "test": "mocha -r ts-node/register src/tests/**/*.ts --reporter spec --timeout 6000",
    "lint": "tslint -c tslint.json src/lib/*.ts src/tests/*.ts src/startupConfigs/*.ts src/frontend/*.ts --fix && eslint ./public-source/ ./public/public-common/mathJaxConfig.js --fix && tslint "
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fhinkel/tryM2.git"
  },
  "devDependencies": {
    "@types/mocha": "2.2.41",
    "@types/node": "8.0.0",
    "@types/socket.io": "1.4.29",
    "@types/ssh2": "0.5.35",
    "@types/mathjax": "0.0.31",
    "@types/socket.io-client": "1.4.29",
    "eslint": "4.0.0",
    "eslint-config-google": "0.5.0",
    "jsdom": "4.3.0",
    "mocha": "2.1.0",
    "rewire": "2.5.1",
    "sinon": "1.14.1",
    "supertest": "0.15.0",
    "ts-node": "3.0.6",
    "tslint": "5.4.3",
    "typescript": "2.3.4",
    "typescript-eslint-parser": "3.0.0",
    "webpack": "1.12.14"
  },
  "eslintConfig": {
    "extends": "google",
    "parser": "typescript-eslint-parser"
  }
}
