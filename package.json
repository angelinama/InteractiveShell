{
  "name": "InteractiveShell",
  "version": "1.1.0",
  "description": "Build web apps for interactive command-line tools",
  "contributors": [
    {
      "name": "Franziska Hinkelmann",
      "email": "franziska.hinkelmann@gmail.com"
    },
    {
      "name": "Lars Kastner",
      "email": "kastner@math.fu-berlin.de"
    },
    {
      "name": "Mike Stillman",
      "email": "mikestillman1@gmail.com"
    }
  ],
  "dependencies": {
    "async": "0.9.0",
    "chai": "2.2.0",
    "cookies": "0.5.0",
    "express": "4.10.8",
    "express-winston": "0.2.9",
    "forever": "0.14.1",
    "serve-favicon": "2.1.7",
    "serve-static": "1.7.2",
    "socket.io": "1.2.1",
    "socket.io-cookie": "0.0.1",
    "socketio-file-upload": "0.2.7",
    "ssh2": "0.4.6",
    "winston": "0.8.3"
  },
  "scripts": {
    "start": "node src/lib/Macaulay2SudoDocker.js",
    "basic": "node src/lib/Macaulay2LocalServer.js",
    "ssh": "node src/lib/Macaulay2SshDocker.js",
    "test": "mocha src/tests/ --reporter spec --timeout 6000",
    "stop": "kill `ps ax | grep Server | grep -v monitor | grep node | awk '{print $1}'`",
    "forever": "./node_modules/forever/bin/forever start -l /home/vagrant/webm2.logs/forever.log -o /home/vagrant/webm2.logs/out.log -e /home/vagrant/webm2.logs/err.log --append src/lib/Macaulay2SshDocker.js",
    // Singular scripts.
    "start_singular": "node src/lib/SingularSudoDocker.js",
    "basic_singular": "node src/lib/SingularLocalServer.js",
    "ssh_singular": "node src/lib/SingularSshDocker.js",
    "forever_singular": "./node_modules/forever/bin/forever start -l /home/user/webm2.logs/forever.log -o /home/user/webm2.logs/out.log -e /home/user/webm2.logs/err.log --append src/lib/Singular2SshDocker.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fhinkel/tryM2.git"
  },
  "devDependencies": {
    "jsdom": "4.3.0",
    "mocha": "2.1.0",
    "rewire": "2.3.1",
    "sinon": "1.14.1",
    "supertest": "0.15.0"
  }
}
